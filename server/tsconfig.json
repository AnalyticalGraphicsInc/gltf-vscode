{
    "compilerOptions": {
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "lib" : [ "es2016" ],
        "outDir": "."
    },
    "exclude": [
        "node_modules"
    ]
}
