{
    "compilerOptions": {
        "alwaysStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "target": "es2017",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "lib" : [ "es2017" ],
        "outDir": "."
    },
    "exclude": [
        "node_modules"
    ]
}
